{
  "name": "magidocql",
  "version": "1.0.0",
  "description": "MagiDocQL is a documentation website generator for GraphQL based on VueJS 3 and that allows for infinite customisation through plugins.",
  "main": "index.js",
  "repository": "git@github.com:pelletier197/magidocql.git",
  "author": "Sunny Pelletier",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,vue}'",
    "test:prettier": "yarn prettier --check '**/*.ts' '**/*.vue'",
    "fix": "run-p fix:*",
    "fix:prettier": "yarn prettier --write '**/*.ts' '**/*.vue'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  }
}

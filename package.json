{
  "name": "magidocql",
  "version": "1.0.0",
  "description": "MagiDocQL is a documentation website generator for GraphQL based on VueJS 3 and that allows for infinite customisation through plugins.",
  "main": "index.js",
  "repository": "git@github.com:pelletier197/magidocql.git",
  "author": "Sunny Pelletier",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "run-p test:*",
    "test:prettier": "yarn prettier --check '**/*.ts' '**/*.vue'",
    "fix": "run-p fix:*",
    "fix:prettier": "yarn prettier --write '**/*.ts' '**/*.vue'"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  }
}

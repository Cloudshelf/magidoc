{
  "name": "@magidoc/cli",
  "description": "A CLI application responsible for generating starter projects for a GraphQL API",
  "private": false,
  "version": "0.1.4",
  "type": "module",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "esbuild --minify --sourcemap --outdir=build ./src/*.ts && tsc --build tsconfig.build.json",
    "test": "jest",
    "release": "pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "esbuild": "^0.14.34",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}

{
  "name": "@magidoc/plugin-prismjs",
  "description": "A plugin that provide a Svelte component any code using PrismJS",
  "type": "module",
  "license": "MIT",
  "version": "0.0.19",
  "svelte": "index.js",
  "private": false,
  "scripts": {
    "build": "pnpm check && pnpm package",
    "package": "svelte-kit package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "release": "pnpm build && cd dist && pnpm publish"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/prismjs": "^1.26.0",
    "graphql": "^16.3.0",
    "prismjs": "^1.27.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "svelte2tsx": "^0.5.5",
    "tslib": "^2.3.1",
    "typescript": "~4.5.5"
  }
}

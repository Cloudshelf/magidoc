  },
  "devDependencies": {
    "codemirror": "^5.65.2",
    "codemirror-graphql": "^1.2.12",
    "graphql": "^16.3.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/codemirror": "^5.60.5",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "svelte2tsx": "^0.5.5",
    "tslib": "^2.3.1",
    "typescript": "~4.5.5"
  }
}

{
  "name": "@magidoc/plugin-code-mirror",
  "description": "A plugin that provide a Svelte component to display GraphQL queries/variables/response",
  "type": "module",
  "license": "MIT",
  "version": "0.1.4",
  "svelte": "index.js",
  "private": false,
  "scripts": {
    "build": "pnpm check && pnpm package",
    "package": "svelte-kit package",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "release": "cd package && pnpm publish --no-git-checks --access public"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/codemirror": "^5.60.5",
    "codemirror": "^5.65.2",
    "codemirror-graphql": "^1.2.15",
    "graphql": "^16.3.0",
    "svelte": "^3.47.0",
    "svelte-check": "^2.5.0",
    "svelte-preprocess": "^4.10.5",
    "svelte2tsx": "^0.5.7",
    "tslib": "^2.3.1",
    "typescript": "~4.5.5"
  }
}
